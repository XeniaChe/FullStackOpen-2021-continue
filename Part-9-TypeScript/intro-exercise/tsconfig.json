{
  "compilerOptions": {
    "noImplicitAny": true, // if false ->  does not require having types for all variables used.
    "target": "ES2022",
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true,
    "esModuleInterop": true
  }
}
